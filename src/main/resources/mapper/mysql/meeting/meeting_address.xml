<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper   PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pmp4.amoimproject.meeting.model.MeetingDAO">
    <insert id="insertMeetingAddress" parameterType="meetingAddressVo">
        insert into MEETING_ADDRESS(
                                    meeting_no,
                                    zonecode,
                                    address,
                                    road_address,
                                    jibun_address,
                                    sido,
                                    sigungu,
                                    bcode,
                                    bname,
                                    place_name,
                                    LON_X,
                                    LAT_Y)
        values (
                #{meetingNo},
                #{zonecode},
                #{address},
                #{roadAddress},
                #{jibunAddress},
                #{sido},
                #{sigungu},
                #{bcode},
                #{bname},
                #{placeName},
                #{lonX},
                #{latY})
    </insert>
</mapper>